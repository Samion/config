 set background=dark
 set guifont=Monaco
 highlight Cursor guibg=Black guifg=NONE
 let g:user_zen_expandabbr_key = '<c-e>'
 let g:user_zen_next_key = '<c-r>'
 set number
 set tabstop=2
  set encoding=utf-8
 set termencoding=utf-8
 colorscheme desert 
 set smartindent
 set backspace=indent,eol,start 
 syntax enable
 filetype on
